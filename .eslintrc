{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-labels": "warn",
        "no-unused-expressions": "warn",
        "no-unused-vars": "warn",
        "no-undef": "off"
    },
}